  "description": "Single page web application template featuring Typescript, React, Redux, Eslint, Prettier, Parcel, Docker",
  "source": "src/index.html",
  "scripts": {
    "prepare": "husky install",
    "start": "parcel src/index.html",
    "build": "parcel build",
    "test": "jest",
    "lint": "eslint --cache --fix --ext .json,.ts,.tsx  src"
  },
  "keywords": [
    "TypeScript",
    "React",
    "Redux",
    "ESLint",
    "Prettier",
    "Parcel",
    "Docker"
  ],
  "author": "hidarikani",
  "license": "ISC",
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "parcel": "^2.10.3",
    "process": "^0.11.10",
    "typescript": "^5.3.3"
